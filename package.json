{
  "name": "gitlab-radiator",
  "author": {
    "name": "heikkipora",
    "email": "heikki.pora@gmail.com"
  },
  "description": "The missing GitLab build radiator view",
  "version": "2.2.1",
  "license": "MIT",
  "bin": {
    "gitlab-radiator": "bin/gitlab-radiator"
  },
  "scripts": {
    "build": "./build-npm",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 babel-node src/app.js",
    "eslint": "eslint --fix src/* bin/* test/*",
    "test": "mocha --require babel-register --require babel-polyfill --timeout 20000 test/*.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heikkipora/gitlab-radiator.git"
  },
  "bugs": {
    "url": "https://github.com/heikkipora/gitlab-radiator/issues"
  },
  "homepage": "https://github.com/heikkipora/gitlab-radiator#readme",
  "contributors": [
    {
      "name": "codegeneralist",
      "email": "antti.oittinen@gmail.com"
    }
  ],
  "dependencies": {
    "axios": "0.18.0",
    "babel-runtime": "6.26.0",
    "basic-auth": "2.0.0",
    "compression": "1.7.2",
    "date-fns": "1.29.0",
    "express": "4.16.3",
    "js-yaml": "3.11.0",
    "lodash": "4.17.5",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "socket.io": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babelify": "8.0.0",
    "browserify": "16.1.1",
    "browserify-middleware": "8.1.0",
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-react": "7.7.0",
    "less-middleware": "2.2.1",
    "mocha": "5.0.5",
    "uglify-js": "3.3.16"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env",
            "react"
          ],
          "plugins": [
            "transform-class-properties"
          ]
        }
      ]
    ]
  }
}
